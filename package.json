    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  }
}
