{
  "name": "react-typescript-starter",
  "version": "1.0.0",
  "description": "React, Typescript, Webpack, Unit Test Starter Kit",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist/*",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts,jsx,tsx}\" --color --fix",
    "start-dev": "webpack-dev-server --config webpack.dev.js --open --hot",
    "start": "npm run start-dev",
    "build": "npm run clean && webpack --config webpack.prod.js",
    "start-prod": "npm run lint npm run build",
    "cypress": "cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tunganh252/React-Typescript-Starter.git"
  },
  "author": "NTA",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tunganh252/React-Typescript-Starter/issues"
  },
  "homepage": "https://github.com/tunganh252/React-Typescript-Starter#readme",
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "cypress": "^4.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^10.0.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/webpack": "^4.41.7",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "csv-loader": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "papaparse": "^5.1.1",
    "postcss-loader": "^3.0.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
